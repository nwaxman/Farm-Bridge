<h2>Add a vendor</h2>
<%= form_for :vendor, :url => vendors_url, :method => :post do |f| -%>
  <p>
    <%= label_tag "Name" %>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= label_tag "Description" %><br />
    <%= f.text_area :description %>
  </p>
  <p>
  <%= submit_tag "Create" %>
  </p>
<% end %>